{
  "name": "shamir-secret-sharing",
  "description": "App for creating and combining secrets with Shamir Secret Sharing",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@consento/shamirs-secret-sharing": "^1.0.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "bootstrap": "^4.6.0",
    "electron-is-dev": "^2.0.0",
    "jquery": "^3.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "wait-on": "^5.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none PORT=3001 npm run start\" \"wait-on http://localhost:3001 && electron .\"",
    "compile": "npm run build && electron-builder"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
